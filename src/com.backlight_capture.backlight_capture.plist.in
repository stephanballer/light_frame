<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.backlight_capture.screen_capture</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/backlight_capture</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/tmp/screen_capture.out</string>
    <key>StandardErrorPath</key>
    <string>/tmp/screen_capture.err</string>
</dict>
</plist>
